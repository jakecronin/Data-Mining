


%% Run KMeans on Iris Dataset
formatSpec = '%d%d%d%d%C';   

errorsOverIterations = runJakeKMeans('irisdata.dat',4,'irisout.dat',true,formatSpec);
figure('NumberTitle', 'off', 'Name', 'Iris Data Errors over Iterations');
plot(errorsOverIterations)

for i = 1:10
    errs(i) = 
end



